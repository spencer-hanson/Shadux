function usage() {
	echo Usage $0 [--install] [--update]
	echo --install installs # to the system
	echo --update updates the data for #
}

if [ -z "$1" ] ; then
	usage
	exit
fi

if [ "$1" = "--update" ] ; then
	echo Updating #...
	cd #
	wget $(../links.sh #)
	mv #*.tar* ../data
	echo Done!
	exit
fi


if [ "$1" = "--install" ] ; then
	echo Installing #...
	cp data/#*.tar* #
	cd #
	#install code



	echo Finished, Cleaning up...
	rm #*.tar*
	rm -r #*
	echo Done!
	exit
fi
usage

openssl
certauth
openssh


gpm
links

linuxpam
shadow

man
